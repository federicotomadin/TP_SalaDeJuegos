          this.Mensajes = mensaje;
          var x = document.getElementById('snackbar');

          if (ganador) {
            x.className = 'show Ganador';
          } else {
            x.className = 'show Perdedor';
          }

          var modelo = this;
          setTimeout(function () {
            x.className = x.className.replace('show', '');
            modelo.ocultarVerificar = false;
          }, 3000);
