        value: function realizarCuenta() {
          var retorno = 0;

          if (this.operador === 0) {
            retorno = this.primerNumero + this.segundoNumero;
          } else if (this.operador === 1) {
            retorno = this.primerNumero - this.segundoNumero;
          } else if (this.operador === 2) {
            retorno = this.primerNumero * this.segundoNumero;
          } else if (this.operador === 3) {
            retorno = this.primerNumero / this.segundoNumero;
          }

          return retorno;
        }
