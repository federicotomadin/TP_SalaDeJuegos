        _classCallCheck(this, ProcesaGrilla);
      }

      _createClass(ProcesaGrilla, [{
        key: "evaluaRespuesta",
        value: function evaluaRespuesta(pMatriz, pMatrizRespuestas) {
          var _this3 = this;

          var contador = 0;
          var encontroPalabra = true;
          pMatrizRespuestas.forEach(function (palabraBuscada) {
            encontroPalabra = true;
            pMatriz.forEach(function (element) {
              for (var index = 0; index < element.length; index++) {
                if (_this3.esRespuesta(element[index], palabraBuscada) === false) {
                  encontroPalabra = false;
                }
              }
            });

            if (encontroPalabra) {
              contador = contador + 1;
            }
          });
          return contador;
        }
      }, {
        key: "esRespuesta",
        value: function esRespuesta(casilla, palabraBuscada) {
          var estaMarcada = true;
          palabraBuscada.forEach(function (element) {
            if (element.id == casilla.id) {
              estaMarcada = casilla.seleccionado;
            }
          });
          return estaMarcada;
        }
      }]);

      return ProcesaGrilla;
    }();
