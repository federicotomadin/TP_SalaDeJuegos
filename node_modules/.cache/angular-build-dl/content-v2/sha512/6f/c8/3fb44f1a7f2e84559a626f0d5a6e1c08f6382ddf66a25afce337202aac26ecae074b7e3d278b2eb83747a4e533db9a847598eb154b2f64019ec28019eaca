      function CabeceraComponent(authService, router) {
        _classCallCheck(this, CabeceraComponent);

        this.authService = authService;
        this.router = router;

        if (localStorage.getItem('token') == '') {
          this.token = false;
        } else {
          this.token = true;
        }

        this.email = localStorage.getItem('email');
      }

